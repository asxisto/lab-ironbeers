<h1>Random Beer</h1>

<ul>
  {{#each randomBeer}}
    <li>
        {{> beerpartial }}
        <br>
        <h4>Food Pairing:</h4>
        <ul>
          {{#each this.food_pairing}}
          <li>{{this}}</li>
          {{/each}}        
        </ul>
        <br>
        <h4>Brewer Tips:</h4>
        <p>{{brewers_tips}}</p>
    </li>
  {{/each}}
</ul>